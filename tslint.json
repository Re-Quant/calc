{
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "rules": {
    "interface-name": false,
    "max-line-length": [
      true,
      100
    ],
    "no-console": [
      true,
      "time",
      "timeEnd",
      "trace"
    ],
    "no-string-literal": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "trailing-comma": [
      false,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "variable-name": [
      true,
      "require-const-for-all-caps",
      "allow-leading-underscore",
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "no-increment-decrement": false,
    "prefer-for-of": false,
    "array-type": false
  }
}
